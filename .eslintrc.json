{
  "env": {
    "jest": true
  },
  "extends": ["next/core-web-vitals", "plugin:jest/recommended"],
  "rules": {
    "react/no-unescaped-entities": ["error", { "forbid": [">", "}"] }],
    "import/no-anonymous-default-export": [
      "error",
      {
        "allowObject": true
      }
    ]
  },
  "plugins": ["jest"]
}
